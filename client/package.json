{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "serve -s ./dist -p 3000",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "test": "NODE_ENV=test jest",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:ci": "npm run build && start-server-and-test start http://localhost:3000 cy:run",
    "format:check": "prettier src --check",
    "format": "prettier src --write",
    "ts:check": "tsc -p .",
    "ts:watch": "npm run ts:check -- --watch"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "express": "^4.18.2",
    "jest": "^29.5.0",
    "lodash": "^4.17.21",
    "numeral": "^2.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.3",
    "react-query": "^3.39.3",
    "serve": "^14.2.0",
    "start-server-and-test": "^2.0.0",
    "styled-components": "^5.3.9",
    "styled-system": "^5.1.5",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.192",
    "@types/numeral": "^2.0.2",
    "@types/react": "^18.0.28",
